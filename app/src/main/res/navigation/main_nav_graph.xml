<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_nav_graph"
    app:startDestination="@id/authFragment">

    <fragment
        android:id="@+id/signUpFragment"
        android:name="com.mobven.fitai.presentation.login.sign_up.SignUpFragment"
        android:label="fragment_sign_up"
        tools:layout="@layout/fragment_sign_up" >
        <action
            android:id="@+id/action_signUpFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_signUpFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/authFragment"
        android:name="com.mobven.fitai.presentation.login.AuthFragment"
        android:label="AuthFragment" >
        <action
            android:id="@+id/action_authFragment_to_signUpFragment"
            app:destination="@id/signUpFragment"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out" />
        <action
            android:id="@+id/action_authFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.mobven.fitai.presentation.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_nutritionFragment"
            app:destination="@id/nutritionFragment" />
        <action
            android:id="@+id/action_homeFragment_to_trainingFragment"
            app:destination="@id/trainingFragment" />
        <action
            android:id="@+id/action_homeFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
    </fragment>
    <fragment
        android:id="@+id/nutritionFragment"
        android:name="com.mobven.fitai.presentation.home.nutrition.NutritionFragment"
        android:label="fragment_nutrition"
        tools:layout="@layout/fragment_nutrition" >
        <action
            android:id="@+id/action_nutritionFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/trainingFragment"
        android:name="com.mobven.fitai.presentation.home.training.TrainingFragment"
        android:label="fragment_training"
        tools:layout="@layout/fragment_training" >
        <action
            android:id="@+id/action_trainingFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.mobven.fitai.presentation.profile.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" >
        <action
            android:id="@+id/action_profileFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.mobven.fitai.presentation.login.sign_in.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_loginFragment_to_signUpFragment"
            app:destination="@id/signUpFragment" />
        <action
            android:id="@+id/action_loginFragment_to_forgotPasswordFragment"
            app:destination="@id/forgotPasswordFragment" />
    </fragment>
    <fragment
        android:id="@+id/resetPasswordFragment"
        android:name="com.mobven.fitai.presentation.login.sign_in.ResetPasswordFragment"
        android:label="fragment_reset_password"
        tools:layout="@layout/fragment_reset_password" >
        <action
            android:id="@+id/action_resetPasswordFragment_to_resetPasswordSuccessFragment"
            app:destination="@id/resetPasswordSuccessFragment" />
    </fragment>
    <fragment
        android:id="@+id/resetPasswordSuccessFragment"
        android:name="com.mobven.fitai.presentation.login.sign_in.ResetPasswordSuccessFragment"
        android:label="fragment_reset_password_succes"
        tools:layout="@layout/fragment_reset_password_succes" >
        <action
            android:id="@+id/action_resetPasswordSuccessFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/forgotPasswordFragment"
        android:name="com.mobven.fitai.presentation.login.sign_in.ForgotPasswordFragment"
        android:label="fragment_forgot_password"
        tools:layout="@layout/fragment_forgot_password" >
        <action
            android:id="@+id/action_forgotPasswordFragment_to_forgotPasswordEmailFragment"
            app:destination="@id/forgotPasswordEmailFragment" />
        <action
            android:id="@+id/action_forgotPasswordFragment_to_OTPFragment2"
            app:destination="@id/OTPFragment" />
    </fragment>
    <fragment
        android:id="@+id/forgotPasswordEmailFragment"
        android:name="com.mobven.fitai.presentation.login.sign_in.ForgotPasswordEmailFragment"
        android:label="fragment_forgot_email"
        tools:layout="@layout/fragment_forgot_password_email" >
        <action
            android:id="@+id/action_forgotPasswordEmailFragment_to_forgotPasswordFragment"
            app:destination="@id/forgotPasswordFragment" />
        <action
            android:id="@+id/action_forgotPasswordEmailFragment_to_OTPEmailFragment2"
            app:destination="@id/OTPEmailFragment2" />
    </fragment>
    <fragment
        android:id="@+id/OTPFragment"
        android:name="com.mobven.fitai.presentation.login.sign_in.OTPFragment"
        android:label="fragment_otp"
        tools:layout="@layout/fragment_otp" >
        <action
            android:id="@+id/action_OTPFragment_to_resetPasswordFragment"
            app:destination="@id/resetPasswordFragment" />
    </fragment>
    <fragment
        android:id="@+id/OTPEmailFragment2"
        android:name="com.mobven.fitai.presentation.login.sign_in.OTPEmailFragment"
        android:label="fragment_otp_email"
        tools:layout="@layout/fragment_otp_email" >
        <action
            android:id="@+id/action_OTPEmailFragment2_to_resetPasswordFragment"
            app:destination="@id/resetPasswordFragment" />
    </fragment>
</navigation>